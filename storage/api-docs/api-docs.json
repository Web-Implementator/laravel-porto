            "get": {
                "tags": [
                    "User"
                ],
                "summary": "Получить пользователя по ID",
                "operationId": "userGetById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID User",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        },
                        "example": "1"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "Service Unavailable",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
