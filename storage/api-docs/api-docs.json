    "paths": {
        "/api/v1/car/all": {
            "get": {
                "tags": [
                    "Car"
                ],
                "summary": "Получение списка автомобилей",
                "operationId": "carGetAll",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "Service Unavailable",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/api/v1/car/{id}": {
            "get": {
                "tags": [
                    "Car"
                ],
                "summary": "Получить автомобиль по ID",
                "operationId": "carGetById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "ID Car",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int64"
                        },
                        "example": "1"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "Service Unavailable",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/api/v1/car/action/rent": {
            "post": {
                "tags": [
                    "Car"
                ],
                "summary": "Аренда автомобиля",
                "operationId": "carActionRent",
                "parameters": [
                    {
                        "name": "car_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "user_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "500": {
                        "description": "Service Unavailable",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/api/v1/car/action/unrent": {
            "post": {
                "tags": [
                    "Car"
                ],
                "summary": "Завершить аренду автомобиля",
                "operationId": "carActionUnRent",
                "parameters": [
                    {
                        "name": "car_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    },
                    {
                        "name": "user_id",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "400": {
                        "description": "Bad Request"
                    },
                    "500": {
                        "description": "Service Unavailable",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
        },
        "/api/v1/user/all": {
            "get": {
                "tags": [
                    "User"
                ],
                "summary": "Получить список пользователей",
                "operationId": "userGetAll",
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {}
                        }
                    },
                    "500": {
                        "description": "Service Unavailable",
                        "content": {
                            "application/json": {}
                        }
                    }
                }
            }
